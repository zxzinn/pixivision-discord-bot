{
	"name": "pixivision-discord-bot",
	"version": "1.0.0",
	"description": "A Discord bot that monitors Pixivision RSS feeds and sends notifications in multiple languages",
	"module": "src/index.ts",
	"type": "module",
	"private": true,
	"scripts": {
		"start": "bun run src/index.ts",
		"dev": "bun --watch src/index.ts",
		"deploy-commands": "bun run src/deploy-commands.ts",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"check": "biome check --write .",
		"railway:setup": "bash scripts/railway-setup.sh",
		"prepare": "husky"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.2",
		"@types/bun": "latest",
		"husky": "^9.1.7"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"@supabase/supabase-js": "^2.78.0",
		"discord.js": "^14.24.2",
		"rss-feed-emitter": "^3.2.3"
	}
}
